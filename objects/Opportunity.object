    <validationRules>
        <fullName>Account_versus_Opportunity_currency</fullName>
        <active>false</active>
        <description>Account and opportunity currency need to be the same</description>
        <errorConditionFormula>TEXT(CurrencyIsoCode) &lt;&gt;  TEXT(Account.CurrencyIsoCode)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>The currency on the account needs to be identical to the one on the opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Agree_Requirement_Definition_Task</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.TurnOffAutomation__c.Validation_Rules__c=true,
Probability &gt;=0.2,
ISBLANK(TEXT(Agree_Requirement_Definition_Task__c)),
AND(RecordType.DeveloperName&lt;&gt;&apos;Innovation_Opportunities_CD&apos;, RecordType.DeveloperName&lt;&gt;&apos;Standard_Opportunities_CD&apos;))</errorConditionFormula>
        <errorDisplayField>Agree_Requirement_Definition_Task__c</errorDisplayField>
        <errorMessage>Please mark this task as complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Avoid_unknown_opportunity_record_type</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.TurnOffAutomation__c.Validation_Rules__c=true,Probability &gt; 0.1,RecordTypeId = &apos;01220000000Uark&apos;)</errorConditionFormula>
        <errorMessage>Please change the opportunity record type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Business_Area</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.TurnOffAutomation__c.Validation_Rules__c=true,OR(Business_Area__r.Name=&quot;&quot;,ISNULL(Business_Area__r.Name)) , Probability &gt;= 0.2  )</errorConditionFormula>
        <errorDisplayField>Business_Area__c</errorDisplayField>
        <errorMessage>Confirm the business area before moving to this probability level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Account_Verified</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.TurnOffAutomation__c.Validation_Rules__c=true,Account_Needs_Verification__c = &apos;YES&apos;, Account_Verified__c = FALSE,  Probability &gt;= 0.2 )</errorConditionFormula>
        <errorDisplayField>Account_Verified__c</errorDisplayField>
        <errorMessage>The Account must  be verified at this probability level</errorMessage>
    </validationRules>
    <validationRules>
