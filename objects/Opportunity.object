    <validationRules>
        <fullName>Account_versus_Opportunity_currency</fullName>
        <active>false</active>
        <description>Account and opportunity currency need to be the same</description>
        <errorConditionFormula>TEXT(CurrencyIsoCode) &lt;&gt;  TEXT(Account.CurrencyIsoCode)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>The currency on the account needs to be identical to the one on the opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Agree_Requirement_Definition_Task</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Probability &gt;=0.2,
ISBLANK(TEXT(Agree_Requirement_Definition_Task__c)),
AND(RecordType.DeveloperName&lt;&gt;&apos;Innovation_Opportunities_CD&apos;, RecordType.DeveloperName&lt;&gt;&apos;Standard_Opportunities_CD&apos;))</errorConditionFormula>
        <errorDisplayField>Agree_Requirement_Definition_Task__c</errorDisplayField>
        <errorMessage>Please mark this task as complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Avoid_unknown_opportunity_record_type</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Probability &gt; 0.1,RecordTypeId = &apos;01220000000Uark&apos;)</errorConditionFormula>
        <errorMessage>Please change the opportunity record type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Business_Area</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(Business_Area__r.Name=&quot;&quot;,ISNULL(Business_Area__r.Name)) , Probability &gt;= 0.2  )</errorConditionFormula>
        <errorDisplayField>Business_Area__c</errorDisplayField>
        <errorMessage>Confirm the business area before moving to this probability level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Account_Verified</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Account_Needs_Verification__c = &apos;YES&apos;, Account_Verified__c = FALSE,  Probability &gt;= 0.2, AND(RecordType.DeveloperName&lt;&gt;&apos;Innovation_Opportunities_CD&apos;, RecordType.DeveloperName&lt;&gt;&apos;Standard_Opportunities_CD&apos;))</errorConditionFormula>
        <errorDisplayField>Account_Verified__c</errorDisplayField>
        <errorMessage>The Account must  be verified at this probability level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Commercial_Proposal</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Allow_Fastrack_Quote__c =false,(Probability &gt;= 0.9), AND(RecordType.DeveloperName&lt;&gt;&apos;Innovation_Opportunities_CD&apos;, RecordType.DeveloperName&lt;&gt;&apos;Standard_Opportunities_CD&apos;),
OR( ISBLANK(  Approval_Commercial_Proposal__c ),Approval_Commercial_Proposal__c   &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Commercial Proposal Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Compliance</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Allow_Fastrack_Quote__c =false,Probability &gt;= 0.9,OR( ISBLANK( Approval_Compliance_certification__c ), Approval_Compliance_certification__c  &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Compliance Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Full_Production_Planning_Schedule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Probability &gt;= 0.9,OR( ISBLANK( Approval_Full_Production_Plan__c ) , Approval_Full_Production_Plan__c  &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Customer Trial and Internal Production Planning Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Know_Your_Customer_Phase_2</fullName>
        <active>false</active>
        <errorConditionFormula>AND( OR(AND(Business_Area__r.MDM_External_Id__c=&apos;FIL&apos;, Probability &gt;= 0.1),AND(Business_Area__r.MDM_External_Id__c=&apos;CSP&apos;, Probability &gt;= 0.9)) ,  Account_Needs_Verification__c = &apos;YES&apos;,
OR(  ISBLANK( Approval_Know_your_customer__c ) ,Approval_Know_your_customer__c &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Know Your Customer Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Requirement_Definition</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  Allow_Fastrack_Quote__c =false,Probability &gt;= 0.2,OR( ISBLANK( Approval_Requirement_definition__c ),  Approval_Requirement_definition__c  &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Agree Requirement Definition Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Supply_Chain_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Probability &gt;= 0.9,OR( ISBLANK(Approval_Supply_chain_validation__c) ,Approval_Supply_chain_validation__c &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Supply Chain Validation Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Trial_Product_Client_Approval</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  Task_Count_Trial_Run__c &gt; 0, Probability &gt;= 0.9,
OR(  ISBLANK(  Approval_Trial_Product_Production__c ) ,Approval_Trial_Product_Production__c &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires the customer to approve the Trial Product Production run Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Value_Proposition</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Allow_Fastrack_Quote__c =false,(Probability &gt;= 0.5),
OR( ISBLANK( Approval_Value_proposition__c ),Approval_Value_proposition__c  &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Create Value Proposition Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_considered_counterfeit</fullName>
        <active>false</active>
        <description>counterfeit</description>
        <errorConditionFormula>AND( Requires_Know_Your_Customer__c = true, Confirmed_Con_counterfeiter__c = false)</errorConditionFormula>
        <errorDisplayField>Confirmed_Con_counterfeiter__c</errorDisplayField>
        <errorMessage>Please confirm you have considered counterfeit activities</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_cost_estimate</fullName>
        <active>false</active>
        <errorConditionFormula>AND(Probability &gt;= 0.7,
OR(ISBLANK(  Approval_Solution_cost_estimate__c  ),  Approval_Solution_cost_estimate__c  &gt; TODAY())
)</errorConditionFormula>
        <errorMessage>This probability level requires an approved Solution Cost Estimate Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_legal_approval</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Allow_Fastrack_Quote__c =false,(Probability &gt;= 0.2),
OR( ISBLANK(Approval_Legal_and_NDA__c),Approval_Legal_and_NDA__c &gt; TODAY()))</errorConditionFormula>
        <errorMessage>This probability level requires an approved Legal / NDA Approval Opportunity Task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_month_split_adds_up</fullName>
        <active>false</active>
        <errorConditionFormula>AND(Probability&gt;=0.8,
Amount_this_year__c &lt;&gt;
 (Revenue_month_1__c +
Revenue_month_2__c +
Revenue_month_3__c +
Revenue_month_4__c +
Revenue_month_5__c +
Revenue_month_6__c +
Revenue_month_7__c +
Revenue_month_8__c +
